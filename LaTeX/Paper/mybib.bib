@article{Byman2006,
	__Markedentry = {[cassydorff]},
	Author = {Byman, D.},
	Date-Added = {2011-06-11 14:59:36 -0400},
	Date-Modified = {2011-06-18 10:06:48 -0400},
	Journal = {Foreign Aff.},
	Keywords = {targeted},
	Owner = {cassydorff},
	Pages = {95},
	Publisher = {HeinOnline},
	Timestamp = {2011.08.24},
	Title = {Do targeted killings work},
	Volume = {85},
	Year = {2006},
	Annote = {Byman discusses the targeted killing of Hamas leader Saleh Shehada
	and the general Israeli policy of targeted killing. He suggests that
	Israel's policy has essentially lead to an escalating tit-for-tat
	interaction as terrorist groups respond to the killings with more
	suicide attacks. Byman claims that the targeted killings perpetrated
	by Israel demonstrate that this tactic is less effective on decentralized
	groups. Thus, terrorist groups in Palestine have adjusted to the
	attacks by becoming more decentralized. Furthermore, such killings
	create martyrs, increase legitimacy for the terrorists' cause, and
	undermine the peace process. However, the policy continues because
	Israel believes the benefits outweigh the costs: the killings boost
	public opinion/morale, while retaliatory attacks may occur, attacks
	over all decrease and the sophistication of these attacks is in decline.
	
	
	Despite any of these advantages, Byman argues, the US should not be
	quick to adopt the use of targeted killings. Firstly, there is a
	US ban on assassination that makes this tactic one built on difficult
	and murky legal territory. Secondly, encouraging the targeted killing
	of other world leaders only encourages the assassination of our own.
	Third, because targeted killings are not widely accepted as a ``legitimate
	instrument of the state'' the US risks its reputation. Lastly, the
	use of targeted killings is much more difficult for the US because
	they face a global threat, not one in a neighboring state. In order
	to use targeted killings as effectively as Israel has the US would
	need to maintain a huge intelligence presence throughout the Middle
	East, Central Asia, and Europe. 
	
	}}

@article{Caplan2006,
	__Markedentry = {[cassydorff]},
	Author = {Caplan, B.},
	Journal = {Public Choice},
	Keywords = {terrorism irrationality paradox of revolution anomalies},
	Note = {ISI Document Delivery No.: 088YH Times Cited: 5 Cited Reference Count: 60 Caplan, Bryan Conference on Political Economy of Terrorism May 24-25, 2005 Arlington, VA Springer Dordrecht},
	Number = {1-2},
	Owner = {cassydorff},
	Pages = {91-107},
	Timestamp = {2011.08.24},
	Title = {Terrorism: The relevance of the rational choice model},
	Volume = {128},
	Year = {2006},
	Abstract = {Terrorism in general, and suicidal terrorism in particular, is popularly
	seen as "irrational," but many economists and political scientists
	argue otherwise. This paper distinguishes three different senses
	of irrationality: unresponsiveness to incentives, deviation from
	narrow self-interest, and failure of rational expectations. It concludes
	that an intermediate position on the rationality of terrorism is
	appropriate. The typical terrorist sympathizer deviates only slightly
	from homo economicus. But active terrorists arguably stray from narrow
	self-interest and rational expectations, and suicidal terrorists
	probably violate both. Deterrence remains a viable anti-terrorism
	strategy, but deviations from rational expectations increase the
	potential of persuasion and appeasement.}}

